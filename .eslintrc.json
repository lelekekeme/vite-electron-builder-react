{
    "root": true,
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": ["plugin:react/recommended", "plugin:react-hooks/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "rules": {
        "react/react-in-jsx-scope": "off"
    }
}
